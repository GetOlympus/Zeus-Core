{
  "name": "getolympus/olympus-zeus-core",
  "type": "framework",
  "description": "Olympus Zeus framework core system used to make all your WordPress plugins and themes developments easier and efficient.",
  "keywords": ["olympus", "zeus", "wordpress", "framework", "php"],
  "homepage": "https://github.com/GetOlympus/Zeus-Core",
  "license": "MIT",
  "authors": [
    {
      "name": "Achraf Chouk",
      "homepage": "https://github.com/crewstyle"
    }
  ],
  "autoload": {
    "psr-4": {
      "GetOlympus\\Zeus\\": "src/Zeus/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "minimum-stability": "stable",
  "require": {
    "behat/transliterator": "^1.0",
    "getolympus/olympus-code-field": "~0.0.1",
    "getolympus/olympus-color-field": "~0.0.1",
    "getolympus/olympus-content-field": "~0.0.1",
    "getolympus/olympus-link-field": "~0.0.1",
    "getolympus/olympus-radio-field": "~0.0.1",
    "getolympus/olympus-select-field": "~0.0.1",
    "getolympus/olympus-text-field": "~0.0.1",
    "getolympus/olympus-textarea-field": "~0.0.1",
    "getolympus/olympus-toggle-field": "~0.0.1",
    "getolympus/olympus-upload-field": "~0.0.1",
    "getolympus/olympus-wordpress-field": "~0.0.1",
    "league/container": "^3.3",
    "symfony/class-loader": "^3.4",
    "symfony/config": "^4.3",
    "symfony/http-foundation": "^4.3",
    "symfony/translation": "^4.3",
    "symfony/yaml": "^4.3",
    "twig/twig": "^2.11"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.2",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "scripts": {
    "test": [
      "phpunit",
      "vendor/bin/phpcs --standard=phpcs.xml ."
    ]
  }
}
