{
  "name": "getolympus/olympus-zeus-core",
  "type": "framework",
  "description": "Olympus Zeus framework core system used to make all your WordPress plugins and themes developments easier and efficient.",
  "keywords": ["olympus", "zeus", "wordpress", "framework", "php"],
  "homepage": "https://github.com/GetOlympus/Zeus-Core",
  "license": "MIT",
  "authors": [
    {
      "name": "Achraf Chouk",
      "homepage": "https://github.com/crewstyle"
    }
  ],
  "autoload": {
    "psr-4": {
      "GetOlympus\\Zeus\\": "src/Zeus/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "minimum-stability": "stable",
  "require": {
    "behat/transliterator": "^1.0",
    "getolympus/olympus-dionysos-field-code": "^0.0.12",
    "getolympus/olympus-dionysos-field-wordpress": "^0.0.17",
    "getolympus/olympus-dionysos-field-upload": "^0.0.14",
    "getolympus/olympus-dionysos-field-toggle": "^0.0.15",
    "getolympus/olympus-dionysos-field-textarea": "^0.0.13",
    "getolympus/olympus-dionysos-field-text": "^0.0.18",
    "getolympus/olympus-dionysos-field-select": "^0.0.11",
    "getolympus/olympus-dionysos-field-radio": "^0.0.16",
    "getolympus/olympus-dionysos-field-link": "^0.0.17",
    "getolympus/olympus-dionysos-field-content": "^0.0.11",
    "getolympus/olympus-dionysos-field-color": "^0.0.12",
    "getolympus/olympus-hera-renderer": "^0.0.2",
    "getolympus/olympus-hermes-translator": "^0.0.2",
    "symfony/class-loader": "^3.4",
    "symfony/http-foundation": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.3",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "scripts": {
    "test": [
      "phpunit",
      "vendor/bin/phpcs --standard=phpcs.xml ."
    ]
  }
}
